@inject IIdentitySetting IdentitySetting

@switch (identity)
{
    case "AspnetIdentity":
        <AspnetIdentityLoginDisplay />
        break;
    case "IdentityServer":
        <IdentityServerLoginDisplay />
        break;
    default:
        break;
}


@code{
    string identity = "";

    protected override void OnInitialized()
    {
        identity = IdentitySetting.Value;
    }

}
